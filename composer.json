{
    "name": "thepublicgood/is-presentable",
    "description": "Simple Laravel model presenter trait",
    "keywords": ["presenter", "presentation", "laravel"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tpg/is-presentable/issues",
        "source": "https://github.com/tpg/is-presentable"
    },
    "authors": [
        {
            "name": "THEPUBLICGOOD",
            "email": "makers@thepublicgood.co.za"
        }
    ],
    "autoload": {
        "psr-4": {
            "TPG\\IsPresentable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TPG\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": ">=8.31"
    },
    "require-dev": {
        "orchestra/testbench": ">=6.13",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "TPG\\IsPresentable\\IsPresentableServiceProvider"
            ]
        }
    }
}
